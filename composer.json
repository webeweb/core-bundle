{
    "name": "webeweb/core-bundle",
    "type": "symfony-bundle",
    "description": "Core bundle for Symfony 4 and more",
    "keywords": [
        "core",
        "bundle",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/core-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/common": "^2.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/security-core": "^4.0",
        "symfony/translation": "^4.0",
        "swiftmailer/swiftmailer": "^5.0|^6.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-library": "^6.8"
    },
    "conflict": {
        "webeweb/bootstrap-bundle": "<3.11",
        "webeweb/haveibeenpwned-bundle": "<1.6",
        "webeweb/smsmode-bundle": "<1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/user-bundle": "^1.3|^2.0",
        "monolog/monolog": "^1.18",
        "symfony/browser-kit": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^2.6|^3.0",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\CoreBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\CoreBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/core-bundle/issues"
    }
}
