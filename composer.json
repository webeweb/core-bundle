{
    "name": "webeweb/core-bundle",
    "type": "symfony-bundle",
    "description": "Core bundle for Symfony 2 and more",
    "keywords": ["webeweb", "bundle", "symfony", "core"],
    "homepage": "http://github.com/webeweb/core-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "webeweb/core-library": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|^7.0"
    },
    "support": {
        "issues": "http://github.com/webeweb/core-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\CoreBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\CoreBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
