{
    "name": "webeweb/core-bundle",
    "type": "symfony-bundle",
    "description": "Core bundle for Symfony 2 and more",
    "keywords": [
        "core",
        "bundle",
        "symfony",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/core-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "doctrine/common": "^2.0",
        "symfony/asset": "^2.7|^3.0|^4.0",
        "symfony/console": "^2.7|^3.0|^4.0",
        "symfony/form": "^2.7|^3.0|^4.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/security-core": "^2.7|^3.0|^4.0",
        "symfony/translation": "^2.7|^3.0|^4.0",
        "swiftmailer/swiftmailer": "^5.0|^6.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/core-library": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^7.0",
        "doctrine/orm": "^2.2",
        "doctrine/doctrine-bundle": "^1.0",
        "friendsofsymfony/user-bundle": "^1.3|^2.0",
        "monolog/monolog": "^1.18",
        "symfony/browser-kit": "^2.7|^3.0|^4.0",
        "symfony/expression-language": "^2.7|^3.0|^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^2.7|^3.0|^4.0",
        "symfony/swiftmailer-bundle": "^2.6|3.0",
        "symfony/templating": "^2.7|^3.0|^4.0",
        "symfony/twig-bundle": "^2.7|^3.0|^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\CoreBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\CoreBundle\\Tests\\": "Tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/core-bundle/issues"
    }
}
